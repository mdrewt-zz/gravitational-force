<h1> Hi Everyone! </h1>

<%= link_to "Sign Up!", new_user_path %>
<%= link_to "Sign In!", users_path %>

<% @attributes.each do |attribute| %>
  <%= form_for(attribute) do |f| %>
    <%= f.label attribute.name %>
    <%= fields_for range_field max: 100, min: 0 %>
    <%= f.check_box "stuff" %>
    <%= f.submit %>
  <% end %>
<% end %>

