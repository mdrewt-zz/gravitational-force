<!DOCTYPE html>
<html>
<head>
  <title>Source</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav>
<% if !logged_in? %>
<%= link_to "Sign Up!", new_user_path %>
<%= link_to "Sign In!", users_path %>
<% end %>

<% if logged_in? %>
<%= link_to "Sign Out!", sign_out_path(current_user)%>
<% end %>
</nav>

<%= yield %>

</body>
</html>
